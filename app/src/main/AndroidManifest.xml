<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pomodoro_timer">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Pomodoro_Timer">
        <service
            android:name=".PlayService"
            android:enabled="true"
            android:exported="true"></service>

        <activity
            android:name=".my_check_list"
            android:exported="true"
            android:label="my_check_list"
            android:theme="@style/Theme.Pomodoro_Timer.NoActionBar" />
        <activity
            android:name=".activity_stop"
            android:exported="true" />
        <activity
            android:name=".Activity_start"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NoteDetailsActivity"
            android:theme="@style/Theme.Pomodoro_Timer.NoActionBar" />
    </application>

</manifest>